<?xml version="1.0" encoding="UTF-8"?>
<web-app>
         
  <!-- Servlets -->
  <servlet>
	<servlet-name>query</servlet-name>
  	<servlet-class>
		movie.db.server.Query
  	</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>query</servlet-name>
	<url-pattern>/error404/myService</url-pattern>
  </servlet-mapping>    

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Error404.html</welcome-file>
  </welcome-file-list>

</web-app>